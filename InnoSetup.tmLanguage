			<string>\[[\w]+\]</string>
			<key>name</key>
			<string>entity.name.section.inno</string>
		</dict>

		<!-- {constants} -->
		<dict>
			<key>match</key>
			<string>{(#)?[\w]+}</string>
			<key>name</key>
			<string>variable.constant.inno</string>
		</dict>

		<!-- %variables -->
		<dict>
			<key>match</key>
			<string>%[\w]</string>
